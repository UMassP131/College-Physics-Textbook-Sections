                <m:mo fence="true" stretchy="true">(</m:mo>
                <m:mrow>
                  <m:mrow>
                    <m:mn>1</m:mn>
                    <m:mrow>
                      <m:mi mathvariant="italic">kW</m:mi>
                      <m:mo stretchy="false">∗</m:mo>
                      <m:mi mathvariant="italic">hr</m:mi>
                    </m:mrow>
                  </m:mrow>
                </m:mrow>
                <m:mo fence="true" stretchy="true">)</m:mo>
              </m:mrow>
              <m:mrow>
                <m:mo fence="true" stretchy="true">(</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:mn>1000</m:mn>
                      <m:mi>W</m:mi>
                    </m:mrow>
                    <m:mi mathvariant="italic">kW</m:mi>
                  </m:mfrac>
                </m:mrow>
                <m:mo fence="true" stretchy="true">)</m:mo>
              </m:mrow>
              <m:mrow>
                <m:mo fence="true" stretchy="true">(</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:mn>1</m:mn>
                      <m:mrow>
                        <m:mi>J</m:mi>
                        <m:mo stretchy="false">/</m:mo>
                        <m:mi>s</m:mi>
                      </m:mrow>
                    </m:mrow>
                    <m:mi>W</m:mi>
                  </m:mfrac>
                </m:mrow>
                <m:mo fence="true" stretchy="true">)</m:mo>
              </m:mrow>
              <m:mrow>
                <m:mrow>
                  <m:mo fence="true" stretchy="true">(</m:mo>
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:mn>3600</m:mn>
                        <m:mi>s</m:mi>
                      </m:mrow>
