                      <m:mo stretchy="false">–</m:mo>
                      <m:mstyle>
                        <m:mrow>
                          <m:mtext fontstyle="italic">mu</m:mtext>
                        </m:mrow><m:mspace width="0.25em"/>
                      </m:mstyle>
                    </m:mrow>
                  </m:mrow>
                  <m:mtext>cos 30º</m:mtext>
                  <m:mtext>; </m:mtext><m:mspace width="0.25em"/>
                  <m:mrow>
                    <m:msub>
