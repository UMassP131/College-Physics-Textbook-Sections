                      </m:msup>
                    </m:mrow>
                    <m:mo stretchy="false">+</m:mo>
                    <m:msub>
                      <m:mi>ν</m:mi>
                      
                        <m:mrow>
                          <m:mi>e</m:mi>
                        </m:mrow>
                      
                    </m:msub>
