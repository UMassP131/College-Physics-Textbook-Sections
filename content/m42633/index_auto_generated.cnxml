                </m:mrow>
              </m:mrow>
            
          </m:mrow>
          <m:annotation encoding="StarMath 5.0"> size 12{"" lSub { size 8{"25"} }  lSup { size 8{"50"} } M rSub { size 8{"25"} }  rightarrow "" lSub { size 8{"24"} }  lSup { size 8{"50"} } "Cr" rSub { size 8{"20"} } +β rSup { size 8{+{}} } +v rSub { size 8{e} } } {}</m:annotation>
        </m:semantics>
