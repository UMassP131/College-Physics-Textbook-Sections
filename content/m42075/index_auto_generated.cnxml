         <m:mo stretchy="false">)</m:mo>
        </m:mrow>
        <m:mi>m</m:mi>
       </m:mfrac>

       <m:mo stretchy="false">=</m:mo>

       <m:mi>g</m:mi><m:mspace width="0.25em"/>
       <m:mtext>sin</m:mtext><m:mspace width="0.25em"/>
       <m:mo stretchy="false">(</m:mo>
       <m:mtext>25º</m:mtext>
       <m:mo stretchy="false">)</m:mo>

   </m:mrow>
  </m:mrow>
 </m:semantics>
</m:math> 
    </equation><equation id="eip-id1165294200578"><m:math>
 <m:semantics>
  <m:mrow>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo>
    <m:mn>9.80</m:mn><m:mspace width="0.25em"/>
     <m:msup>
     <m:mtext> m/s</m:mtext>
     <m:mrow>
      <m:mn>2</m:mn>
     </m:mrow>
    </m:msup>
    <m:mo stretchy="false">)</m:mo>
