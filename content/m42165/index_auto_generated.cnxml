                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                </m:mrow><m:mspace width="0.25em"/>
                <m:mtext> cos </m:mtext><m:mspace width="0.25em"/>
                <m:mrow>
                  <m:msub>
                    <m:mi>θ</m:mi>
                    <m:mrow>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:mo stretchy="false">+</m:mo>
                  <m:msub>
                    <m:mi>v</m:mi>
                    <m:mrow>
                      <m:mn>2</m:mn>
                    </m:mrow>
                  </m:msub>
                </m:mrow><m:mspace width="0.25em"/>
                <m:mtext> cos </m:mtext><m:mspace width="0.25em"/>
                <m:msub>
                  <m:mi>θ</m:mi>
                  <m:mrow>
                    <m:mn>2</m:mn>
                  </m:mrow>
                </m:msub>
              </m:mrow>
            </m:mrow>
          </m:mrow>
        </m:semantics>
      </m:math>
    </equation>
<para id="eip-id2705075">and</para>

<equation id="eip-id1899459"><m:math>
<m:semantics>
<m:mrow><m:mrow><m:mrow><m:mrow><m:mn>0</m:mn><m:mo stretchy="false">=</m:mo><m:msubsup><m:mrow><m:mi>v</m:mi><m:mo>′</m:mo></m:mrow><m:mrow><m:mn>1</m:mn></m:mrow></m:msubsup></m:mrow><m:mspace width="0.25em"/><m:mtext>sin</m:mtext><m:mspace width="0.25em"/><m:mrow><m:msub><m:mi>θ</m:mi><m:mrow><m:mn>1</m:mn></m:mrow></m:msub><m:mo stretchy="false">+</m:mo><m:msubsup><m:mrow><m:mi>v</m:mi><m:mo>′</m:mo></m:mrow><m:mrow><m:mn>2</m:mn></m:mrow></m:msubsup></m:mrow><m:mspace width="0.25em"/><m:mtext>sin</m:mtext><m:mspace width="0.25em"/><m:msub><m:mi>θ</m:mi><m:mrow><m:mn>2</m:mn></m:mrow></m:msub></m:mrow><m:mo>.</m:mo></m:mrow><m:mrow/></m:mrow>
</m:semantics>
</m:math></equation><para id="eip-id2705141">Square each equation to get</para> 
<equation id="eip-id2304354"><m:math>
        <m:semantics>
<m:mtable columnalign="left">
 <m:mtr>
  <m:mtd>    <m:msup> <m:msub>
                    <m:mi>v</m:mi>
                      <m:mn>1</m:mn>
                    </m:msub><m:mn>2</m:mn>
                  </m:msup>
</m:mtd>
<m:mtd>
                  <m:mo stretchy="false">=</m:mo>
</m:mtd>
<m:mtd>
                  <m:msup><m:msub><m:mrow>
                    <m:mi>v</m:mi><m:mo>′</m:mo></m:mrow>
                      <m:mn>1</m:mn></m:msub>
                    <m:mn>2</m:mn>
                  </m:msup>
                <m:mspace width="0.25em"/>
                <m:msup><m:mtext> cos </m:mtext><m:mn>2</m:mn></m:msup><m:mspace width="0.25em"/>
                <m:mrow>
                  <m:msub>
                    <m:mi>θ</m:mi>
