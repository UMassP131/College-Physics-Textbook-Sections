                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:mo stretchy="false">+</m:mo>
                   <m:msup><m:msub><m:mrow>
                    <m:mi>v</m:mi><m:mo>′</m:mo></m:mrow>
                      <m:mn>2</m:mn>
                    </m:msub><m:mn>2</m:mn>
                  </m:msup>
                </m:mrow><m:mspace width="0.25em"/>
               <m:msup><m:mtext> cos </m:mtext><m:mn>2</m:mn></m:msup><m:mspace width="0.25em"/>
                <m:msub>
                  <m:mi>θ</m:mi>
                  <m:mrow>
                    <m:mn>2</m:mn>
                  </m:mrow>
                </m:msub>
                <m:mo stretchy="false">+</m:mo>
                <m:mn>2</m:mn>
                <m:msub>
                  <m:mrow>
                    <m:mi>v</m:mi><m:mo>′</m:mo>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>1</m:mn>
                  </m:mrow>
                </m:msub>
                <m:msub>
                  <m:mrow>
                    <m:mi>v</m:mi><m:mo>′</m:mo>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>2</m:mn>
                  </m:mrow>
